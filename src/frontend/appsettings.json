{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "http://*:5000",
        "Protocols": "Http2"
      },
      "Https": {
        "Url": "https://*:5001",
        "Protocols": "Http1AndHttp2"
      },
      "Http": {
        "Url": "http://*:5002",
        "Protocols": "Http1"
      }
    }
  },
  "AllowedHosts": "*"
}
